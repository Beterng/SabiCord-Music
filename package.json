{
  "name": "sabi-core",
  "version": "2.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/main.js",
    "scripts": {
    "build:debug": "tsc --noEmitOnError false",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:test": "ts-node --transpile-only -r tsconfig-paths/register src/index.ts",
    "watch": "nodemon src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t sabicord-music .",
    "docker:run": "docker-compose up -d"
  },
  "keywords": [
    "discord",
    "bot",
    "music",
    "lavalink",
    "typescript",
    "nodejs"
  ],
  "dependencies": {
    "@discordx/di": "^3.3.4",
    "@discordx/importer": "^1.3.3",
    "@discordx/pagination": "^4.0.1",
    "axios": "^1.6.5",
    "canvas": "^3.1.2",
    "discord.js": "^14.21.0",
    "discordx": "^11.12.5",
    "dotenv": "17.2.0",
    "humanize-duration": "^3.31.0",
    "module-alias": "^2.2.3",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.4",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@discordjs/voice": "^0.18.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.1.0",
    "@types/validator": "^13.11.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "30.0.5",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.3",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChocoMeow/Vocard.git"
  },
  "bugs": {
    "url": "https://github.com/ChocoMeow/Vocard/issues"
  },
  "homepage": "https://github.com/ChocoMeow/Vocard#readme",
  "_moduleAliases": {
    "@audio": "./dist/audio",
    "@core": "./dist/core",
    "@commands": "./dist/commands",
    "@services": "./dist/services",
    "@interfaces": "./dist/interfaces",
    "@views": "./dist/views",
    "@ipc": "./dist/ipc"
  }
}
